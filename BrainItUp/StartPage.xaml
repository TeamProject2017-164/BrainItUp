<Page x:Class="BrainItUp.StartPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:BrainItUp"
      mc:Ignorable="d" 
      d:DesignHeight="600" d:DesignWidth="800"
      Title="Brain It Up" Background="AliceBlue">

    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
            <RowDefinition Height="*"/>
        </Grid.RowDefinitions>
        <Button x:Name="StartButton" Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="184,44,148,45" Width="468" Height="111" FontSize="50" Click="StartButton_Click">Start!</Button>
        <Button x:Name="RatingButton" Grid.Row="2" VerticalAlignment="Center" HorizontalAlignment="Center" Margin="184,44,148,45" Width="468" Height="111" FontSize="50" Click="RatingButton_Click">Ratings</Button>
    </Grid>
</Page>
